<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="android.support.design.widget.BottomSheetBehavior"
    android:background="@color/cardview_light_background"
    >

  <com.github.mikephil.charting.charts.LineChart
      android:id="@+id/lineChart5"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginBottom="4dp"
      android:background="@android:color/holo_orange_dark"
      app:layout_constraintBottom_toTopOf="@+id/lineChart6"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

  <com.github.mikephil.charting.charts.LineChart
      android:id="@+id/lineChart6"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginBottom="4dp"
      android:layout_marginTop="4dp"
      android:background="@android:color/holo_blue_dark"
      app:layout_constraintBottom_toTopOf="@+id/lineChart7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/lineChart5"
      />

  <com.github.mikephil.charting.charts.LineChart
      android:id="@+id/lineChart7"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginBottom="4dp"
      android:layout_marginTop="4dp"
      android:background="@android:color/holo_purple"
      app:layout_constraintBottom_toTopOf="@+id/lineChart8"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/lineChart6"
      />

  <com.github.mikephil.charting.charts.LineChart
      android:id="@+id/lineChart8"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginBottom="4dp"
      android:layout_marginTop="4dp"
      android:background="@android:color/holo_red_dark"
      app:layout_constraintBottom_toTopOf="@+id/lineChart9"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/lineChart7"
      />

  <com.github.mikephil.charting.charts.LineChart
      android:id="@+id/lineChart9"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginTop="4dp"
      android:background="@android:color/holo_green_dark"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/lineChart8"
      />

</android.support.constraint.ConstraintLayout>